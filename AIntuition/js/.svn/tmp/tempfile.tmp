function businessArea(id){
	
	var items = new Array();
	
	$("input[name='businessArea[]']:checked").each(function(){
		items.push(this.value);
	});
	
	var bArea = $(".businessArea").is(":checked");
	
	console.log(items);
	var sendData = {items: items};
	$.ajax({
		url: "/AIntuition/ajax/category/businessUnit.php?id=" + id,
		type: "POST",
		data: sendData,
		beforeSend: function(){cursorLoaderShow();},
		complete: function(){cursorLoaderHide();},
		success: function(data){
			$('#displayBusinessUnit' + id).html(data);
		    $('#displayBusinessUnit' + id).show();
		}      
	});
	
	// Check condition if Business Area is checked:
	
	if(bArea === true){ // Condition if anyone BA is true:
		$.ajax({
					url: '/AIntuition/ajax/category/statusAIntuition.php?bArea=' + bArea + '&id=' + id, // Specifies the URL to sent the request to.
					type: 'POST', // Specifies type of request.  (GET or POST)
					data: {bArea: bArea, id: id}, // Specifies data to be sent to the server.
					contet: document.body,
					success: function(data){ // A function to be run when the request succeed.
						$('#displayStatus' + id).html(data);
						$('#displayStatus' + id).show();
					}
				});	
				
	} else {
		$('#holdingIntuitionArea' + id).show();
		$('#displayStatus' + id).hide();
		
	}
	
	
}

